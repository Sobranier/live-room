<!--index.wxml-->
<view class="container">
  <mlvb-live-room
    id="id_liveroom"
    wx:if="{{showLiveRoom}}"
    roomid="{{roomID}}"
    role="{{role}}"
    roomname="{{roomName}}"
    pureaudio="{{pureAudio}}"
    debug="{{debug}}"
    muted="{{muted}}"
    beauty="{{beauty}}"
    template="float"
    bindRoomEvent="onRoomEvent"
  />
</view>
